//5
   class Square{
       constructor(width) {
           this.width = width;
           this.height = width;
       }
       get area(){
           return this.height * this.width;
       }
       set area(area){
           this.width = Math.sqrt(area);
           this.height = Math.sqrt(area);
       }
   }
   let square1 = new Square(5);
   console.log(square1.area);
   console.log(square1.width);
   square1.area = 64;
   console.log(square1.width);



